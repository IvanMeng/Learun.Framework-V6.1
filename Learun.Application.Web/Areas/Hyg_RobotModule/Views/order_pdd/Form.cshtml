@{
    ViewBag.Title = "拼多多订单列表";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单编号<font face="宋体">*</font></div>
        <input id="order_sn" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">商品id<font face="宋体">*</font></div>
        <input id="goods_id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">商品名称<font face="宋体">*</font></div>
        <input id="goods_name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">商品缩略图<font face="宋体">*</font></div>
        <input id="goods_thumbnail_url" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">商品数量<font face="宋体">*</font></div>
        <input id="goods_quantity" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">商品价格（分）<font face="宋体">*</font></div>
        <input id="goods_price" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单价格（分）<font face="宋体">*</font></div>
        <input id="order_amount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">佣金比例 千分比<font face="宋体">*</font></div>
        <input id="promotion_rate" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">佣金（分）<font face="宋体">*</font></div>
        <input id="promotion_amount" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">结算批次号<font face="宋体">*</font></div>
        <input id="batch_no" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单状态<font face="宋体">*</font></div>
        <input id="order_status" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单状态描述（ -1 未支付; 0-已支付；1-已成团；2-确认收货；3-审核成功；4-审核失败（不可提现）；5-已经结算；8-非多多进宝商品（无佣金订单）;10-已处罚）<font face="宋体">*</font></div>
        <input id="order_status_desc" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单创建时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_create_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单支付时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_pay_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单成团时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_group_success_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单确认收货时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_receive_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单审核时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_verify_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单结算时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_settle_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单最后更新时间（UNIX时间戳）<font face="宋体">*</font></div>
        <input id="order_modify_at" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">订单类型：0：领券页面， 1： 红包页， 2：领券页， 3： 题页<font face="宋体">*</font></div>
        <input id="type" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">成团编号<font face="宋体">*</font></div>
        <input id="group_id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">多多客工具id<font face="宋体">*</font></div>
        <input id="auth_duo_id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">招商多多客id<font face="宋体">*</font></div>
        <input id="zs_duo_id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">自定义参数<font face="宋体">*</font></div>
        <input id="custom_parameters" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">CPS_Sign<font face="宋体">*</font></div>
        <input id="cps_sign" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">链接最后一次生产时间<font face="宋体">*</font></div>
        <input id="url_last_generate_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">打点时间<font face="宋体">*</font></div>
        <input id="point_time" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">售后状态：0：无，1：售后中，2：售后完成<font face="宋体">*</font></div>
        <input id="return_status" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">推广位id<font face="宋体">*</font></div>
        <input id="p_id" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">是否是 cpa 新用户，1表示是，0表示否<font face="宋体">*</font></div>
        <input id="cpa_new" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/Hyg_RobotModule/Views/order_pdd/Form.js")
